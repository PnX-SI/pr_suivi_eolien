{ 
  "display_properties": [
    "id_base_site",
    "observers_txt",
    "visit_date_min",
    "visit_date_max",
    "type_donnee_gite"
  ],
  "display_list": [
    "visit_date_min",
    "nb_observations",
    "type_donnee_gite"
  ],
  "map_label_field_name": "base_site_code",
  "specific": {
    "observers": {
      "required": false,
      "hidden": true
    },
    "observers_txt": {
      "required": false,
      "hidden": true
    },
    "id_dataset": {
      "hidden": "({meta}) => meta.dataset && Object.keys(meta.dataset).length == 1"
    },
    "visit_date_min": {
      "type_widget": "date",
      "attribut_label": "Date de début de la visite",
      "required": true
    },
    "visit_date_max": {
      "type_widget": "date",
      "attribut_label": "Date de fin de la visite",
      "required": false
    },
    "type_donnee_gite": {
      "type_widget": "nomenclature",
      "attribut_label": "type_donnee_gite",
      "code_nomenclature_type": "TYPE_DONNEE_GITE",
      "definition": "Type de données récoltées.",
      "type_util": "nomenclature",
      "required": true,
      "hidden": false
    }
  },
  "change": [
    "({objForm, meta}) => {",
    "const journee_date_fin = objForm.value.visit_date_min;",
    "if (objForm.controls.visit_date_min.valid && !objForm.controls.visit_date_max._pendingDirty) {",
    "objForm.patchValue({'visit_date_max':journee_date_fin})",
    "}",
    "}",
    ""
  ]
}