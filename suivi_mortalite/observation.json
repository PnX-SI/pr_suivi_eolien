{
  
  "display_properties": [
    "id_nomenclature_presence_cadavre",
    "geometrie",
    "distance_eolienne",
    "heure_obs",
    "hauteur_veg",
    "cd_nom",
    "liste_cd_nom",
    "id_nomenclature_sex",
    "identifiant_cadavre",
    "id_nomenclature_presence_foret"
  ],
  "display_list": [
    "id_nomenclature_presence_cadavre",
    "distance_eolienne",
    "heure_obs",
    "cd_nom",
    "liste_cd_nom",
    "id_nomenclature_sex",
    "identifiant_cadavre",
    "id_nomenclature_presence_foret"
  ],
  "map_label_field_name": "base_site_code",
  "redirect_to_parent": true,
  "specific": {
    "id_nomenclature_presence_cadavre": {
      "type_widget": "nomenclature",
      "attribut_label": "Présence de cadavre",
      "code_nomenclature_type": "PRES_CADAVRE",
      "type_util": "nomenclature",
      "required": true,
      "hidden": false
    },
    "geometrie": {
      "type_widget": "text",
      "attribut_label": "Géométrie WKT",
      "definition": "Emplacement géographique du cadavre. Norme ISO/IEC 13249-3:2016. Les points sont les seuls types de géométrie acceptés. Exemple : POINT(5.35 48.41)",
      "required": true
    },
    "distance_eolienne": {
      "type_widget": "number",
      "attribut_label": "Distance à l'éolienne la plus proche.",
      "definition": "Distance en mètre ",
      "required": true,
      "multiple": false,
      "min": 0
    },
    "heure_obs": {
      "type_widget": "time",
      "attribut_label": "Heure à laquelle le cadavre a été découvert.",
      "required": true,
      "multiple": false
    },
    "hauteur_veg": {
      "type_widget": "number",
      "attribut_label": "Hauteur de la végétation",
      "definition": "Hauteur de la végétation herbacée à buissonnante à l'emplacement  du cadavre en centimètres",
      "required": true,
      "multiple": false,
      "min": 0
    },
    "identifiant_cadavre": {
      "type_widget": "number",
      "attribut_label": "Numéro du cadavre servant au test de persitance.",
      "definition": "En prévision du test de persistance, une ligne par cadavre et pas test devra être renseignée.",
      "required": true,
      "multiple": false,
      "min": 0
    },
    "cd_nom": {
      "required": "({value, meta}) => ((meta.nomenclatures[value.id_nomenclature_presence_cadavre] || {}).cd_nomenclature == '1')",
      "hidden": "({value, meta}) =>  !((meta.nomenclatures[value.id_nomenclature_presence_cadavre] || {}).cd_nomenclature == '1')"
    },
    "liste_cd_nom": {
      "type_widget": "datalist",
      "attribut_label": "Liste des codes des taxons possibles dans le cas d'un cadavre pour laquelle une identification dans le champ cdNom ne serait possible qu'à la famille.",
      "keyValue": "cd_nom",
      "keyLabel": "__MODULE.TAXONOMY_DISPLAY_FIELD_NAME",
      "multiple": true,
      "api": "taxref/allnamebylist/__MODULE.ID_LIST_TAXONOMY",
      "application": "TaxHub",
      "type_util": "taxonomie",
      "required": "({value, meta}) => ((meta.nomenclatures[value.id_nomenclature_presence_cadavre] || {}).cd_nomenclature == '1')",
      "hidden": "({value, meta}) =>  !((meta.nomenclatures[value.id_nomenclature_presence_cadavre] || {}).cd_nomenclature == '1')"
    },
    "id_nomenclature_sex": {
      "type_widget": "nomenclature",
      "attribut_label": "Sexe du cadavre retrouvé.",
      "code_nomenclature_type": "SEXE",
      "type_util": "nomenclature",
      "required": "({value, meta}) => (meta.nomenclatures[value.id_nomenclature_presence_cadavre] || {}).cd_nomenclature == '1'",
      "hidden": "({value, meta}) => (meta.nomenclatures[value.id_nomenclature_presence_cadavre] || {}).cd_nomenclature !== '1'"
    },
    "id_nomenclature_presence_foret": {
      "type_widget": "nomenclature",
      "attribut_label": "Localisation du cadavre ou du leurre en forêt ou non",
      "code_nomenclature_type": "PRES_FORET",
      "type_util": "nomenclature",
      "required": true,
      "hidden": false
    },
    "medias": {
      "hidden": true,
      "type_widget": "medias",
      "attribut_label": "Médias",
      "schema_dot_table": "gn_monitoring.t_observations"
    }
  },
  "change": [
    "({objForm, meta}) => {", 
    "if ( !((meta.nomenclatures[objForm.controls['id_nomenclature_presence_cadavre'].value] || {}).cd_nomenclature == '1')) {",
    "objForm.patchValue( {'cd_nom': { cd_nom: 186233, nom_valide: 'Chiroptera Blumenbach, 1779',nom_vern: null }, 'liste_cd_nom': [ ], 'id_nomenclature_sex':null });",
    "}",
    "}"
  ]
}
